# '.', '/' left out

  allchars='! " # $ % & '\'' ( ) * + , - 0 9 : ; < = > ?'	     # emacs: "
  allchars="$allchars"' @ A Z [ \ ] ^ _ ` a z { | } ~'


mkdir d
echo 'd' > subdirs
> subdir_files
set -f
for dir in $allchars; do
  mkdir "d/$dir"
  echo "d/$dir" >> subdirs
  for file in $allchars; do
    touch "d/$dir/$file"
    echo "d/$dir/$file" >> subdir_files
  done
done
