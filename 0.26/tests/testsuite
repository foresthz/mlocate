$at_failed && at_fn_log_failure
$at_traceon; }


kill -KILL $bgpid
wait $bgpid

{ set +x
$as_echo "$at_srcdir/updatedb.at:233: updatedb -U \"\$(pwd)/d\" -o db -l 0"
